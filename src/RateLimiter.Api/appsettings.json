{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimiting": {
    "FailOpen": true,
    "CleanupIntervalSeconds": 300,
    "Rules": {
      "/api/resource": {
        "Limit": 10,
        "Window": "00:01:00",
        "BucketCapacity": 10,
        "RefillRate": 0.16667
      }
    }
  }
}
